# MQTT Configuration
MQTT_ENABLE=true
MQTT_BROKER_HOST=192.168.0.151
MQTT_BROKER_PORT=1883
MQTT_CLIENT_ID=Backend_Client_Dev
MQTT_USERNAME=
MQTT_PASSWORD=
MQTT_USE_TLS=false

# Sensor Data Configuration
# Interval untuk menyimpan data sensor (dalam detik)
# Opsi: 1 menit, 15 menit, 30 menit, 60 menit
# Untuk 1 menit: SENSOR_DATA_SAVE_INTERVAL=60
# Untuk 15 menit: SENSOR_DATA_SAVE_INTERVAL=900
# Untuk 30 menit: SENSOR_DATA_SAVE_INTERVAL=1800
# Untuk 60 menit: SENSOR_DATA_SAVE_INTERVAL=3600
SENSOR_DATA_SAVE_INTERVAL=60
# Minimum interval antara save untuk device yang sama (dalam detik)
SENSOR_DATA_MIN_INTERVAL=60

ENABLE_SEED_DATA=true

# JWT Configuration (Optional - overrides appsettings.json)
# JWT_SECRET_KEY=your-super-secret-key-here
# JWT_ISSUER=Backend
# JWT_AUDIENCE=BackendUsers

# Database (Optional)
# ConnectionStrings__DefaultConnection=Data Source=app.db