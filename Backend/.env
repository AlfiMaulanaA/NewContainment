# MQTT Configuration
MQTT_ENABLE=true
MQTT_BROKER_HOST=192.168.0.138
MQTT_BROKER_PORT=1883
MQTT_CLIENT_ID=Backend_Client_Dev
MQTT_USERNAME=
MQTT_PASSWORD=
MQTT_USE_TLS=false

# Sensor Data Configuration
