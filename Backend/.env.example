# MQTT Configuration
MQTT_ENABLE=false
MQTT_BROKER_HOST=192.168.0.138
MQTT_BROKER_PORT=1883
MQTT_CLIENT_ID=Backend_Client
MQTT_USERNAME=
MQTT_PASSWORD=
MQTT_USE_TLS=false

# Sensor Data Configuration
# Interval untuk menyimpan data sensor (dalam detik)
SENSOR_DATA_SAVE_INTERVAL=60
# Minimum interval antara save untuk device yang sama (dalam detik)
SENSOR_DATA_MIN_INTERVAL=60

# Database Seeding Configuration
# Set to true to populate database with sample data on startup
# Set to false to start with empty database
ENABLE_SEED_DATA=true

# JWT Configuration (Optional - overrides appsettings.json)
JWT_SECRET_KEY=your-super-secret-key-here-must-be-at-least-256-bits
JWT_ISSUER=Backend
JWT_AUDIENCE=BackendUsers

# Database (Optional)
ConnectionStrings__DefaultConnection=Data Source=app.db
